@model List<DemoPortfolioProject.Models.Entities.TBLExperience>
<section class="w3l-homeblock3" id="awards">
    <div class="midd-w3 py-5">
        <div class="container py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-6">
                    <span class="text">İş Deneyimlerim</span>


                    <div class="reward-list mt-5">
                        @foreach (var item in Model)
                        {

                            <div style="color:brown">
                                <strong> @item.StartDate.Value.Year-@item.StartDate.Value.Month \                                    @item.EndDate.Value.Year-@item.EndDate.Value.Month</strong>
                            </div>
                            <div class="reward-item-detail">
                                <span class="reward-item-title">@item.CompanyNamme</span>
                                <p class="reward-item-descr">@item.Description</p>
                            </div>
                            <br />

                        }





                    </div>
                </div>
                <div class="col-lg-6 align-self mt-lg-0 mt-md-5 mt-4">
                    <div class="position-relative px-lg-5 mx-lg-0 mx-md-5 image-padding">
                        <img src="~/Template/iportfolio-starter/assets/images/awards.png" alt="" class="radius-image img-fluid">
                        <div class="position-absolute-images">
                            
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>

